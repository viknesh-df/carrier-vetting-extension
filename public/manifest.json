{
  "manifest_version": 3,
  "name": "Carrier Vetting Agent",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting", "sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://pangents.deepfrog.ai/*"],
      "js": ["content.js"]
    }
  ],
  "action": { "default_title": "Carrier Vetting Agent" }, 
  "side_panel": { "default_path": "index.html" }
}
